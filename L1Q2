            System.out.println("Matric number: 22004781/1");
            System.out.println("Address\t\t : No. 14, Jalan Dato Yusuf Shahbudin 1, Taman Sentosa, 41200 Klang, Selangor, Malaysia.");
            System.out.println("Email\t\t : leepeihui.lph@gmail.com");
            System.out.println("Contact\t\t : 016-267 0910");
        }
}

